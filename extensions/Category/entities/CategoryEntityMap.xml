<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE entity SYSTEM
    "https://github.com/nibralab/joomla-architecture/blob/master/code/Joomla/ORM/Definition/entity.dtd">
<entity name="Joomla\Extension\Category\Entity\CategoryEntityMap">

    <storage>
        <default table="category_entity_map"/>
    </storage>

    <fields>
        <field name="entity_type"
               type="entity_type"
        />

        <field name="entity_id"
               type="integer"
        />
    </fields>

    <relations>
        <belongsTo name="category_id"
                   entity="Category"
        />
    </relations>

</entity>
